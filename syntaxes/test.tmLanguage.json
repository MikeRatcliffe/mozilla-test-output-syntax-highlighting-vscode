{
  "fileTypes": ["test"],
  "name": "Mozilla Test Output",
  "patterns": [
    {
      "match": "^\\s*\\d?\\d:\\d\\d[:.]\\d\\d",
      "name": "comment"
    },
    {
      "match": "\\bINFO\\b",
      "name": "markup.inserted"
    },
    {
      "match": "\\bTEST_START|TEST_END|TEST_KNOWN_FAIL|SUITE_START|SUITE_END\\b",
      "name": "entity.name.type.class"
    },
    {
      "match": "\\bTEST-INFO\\b",
      "name": "string"
    },
    {
      "match": "\\bEMITTING:",
      "name": "constant.numeric"
    },
    {
      "match": "WARNING:?",
      "name": "entity.name.function"
    },
    {
      "match": "(\\+\\+|--).*",
      "name": "comment"
    },
    {
      "match": "\\bJavaScript strict warning:.*",
      "name": "entity.name.function"
    },
    {
      "match": "\\[(JavaScript Warning:.+)\\]",
      "captures": {
        "1": {
          "name": "entity.name.function"
        }
      }
    },
    {
      "match": "([\\w\\.\\/\\<]*)(@)(.+)(:)(\\d+:\\d+)",
      "captures": {
        "1": {
          "name": "keyword"
        },
        "2": {
          "name": "constant.numeric.js"
        },
        "3": {
          "name": "keyword"
        },
        "4": {
          "name": "keyword.operator"
        },
        "5": {
          "name": "constant.numeric.js"
        }
      }
    },
    {
      "match": "\\bconsole.warn:.*",
      "name": "entity.name.function"
    },
    {
      "match": "\\bJavaScript warning:.*",
      "name": "entity.name.function"
    },
    {
      "match": "(.*)(TEST-PASS)(.*)",
      "captures": {
        "1": {
          "name": "comment"
        },
        "2": {
          "name": "entity.name.function"
        },
        "3": {
          "name": "text"
        }
      }
    },
    {
      "match": "(.*)(TEST-INFO)(.*exceptions.*)",
      "captures": {
        "1": {
          "name": "comment"
        },
        "2": {
          "name": "string"
        },
        "3": {
          "name": "text"
        }
      }
    },
    {
      "match": ".*\\b[Ee]xception\\b.*",
      "name": "invalid"
    },
    {
      "match": "(JavaScript [Ee]rror:|InvalidStateError|TEST-UNEXPECTED-FAIL|TEST-UNEXPECTED-PASS|DEBUG:|Stack trace:|Stack:|console.error).*",
      "name": "invalid"
    },
    {
      "match": "(GECKO)(\\()(24592)(\\))",
      "captures": {
        "1": {
          "name": "keyword.control"
        },
        "2": {
          "name": "keyword.control"
        },
        "3": {
          "name": "constant.numeric.js"
        },
        "4": {
          "name": "keyword.control"
        }
      }
    }
  ],
  "scopeName": "source.mozillatestoutput"
}
